        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "content": {
      "type": "blocks"
    },
    "publishedDate": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "order": {
      "type": "integer"
    }
  }
